package com.promineotech.flight.dao;

import java.util.List;

import com.promineotech.flight.entity.FlightReview;


public interface FlightReviewInterfaceDao {
	List<FlightReview> fetchFlightReviews();

	List<FlightReview> fetchFlightReviews(int flightReviewId);

	List<FlightReview> fetchFlightReviews(int flightReviewId, int flightId, int passengerId, Boolean wasOntime,
			Boolean seatWasClean);
}
